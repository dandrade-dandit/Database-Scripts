<a:Text>Create Trigger T_INF_TRI On T_INF
For Insert As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Regra de negócio */
/* Só pode existir um único Índice Financeiro Padrão */
IF (	SELECT 	COUNT(*) 
	FROM 	T_INF INF,
		INSERTED INS
	WHERE 	INF.CD_INF &lt;&gt; INS.CD_INF AND
		INF.FL_INF_PAD = 1 AND
		INS.FL_INF_PAD = 1) &gt; 0
BEGIN
	RAISERROR 20310 &#39;Operação não executada: Só pode existir um único Índice Financeiro Padrão.&#39;
	ROLLBACK TRANSACTION
	RETURN

END
End
go</a:Text>
