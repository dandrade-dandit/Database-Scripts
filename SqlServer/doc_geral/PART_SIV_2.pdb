		FROM 	T_CHK
		WHERE 	CD_CHK &lt;&gt; @CD_CHK AND NR_CHK_ORD = @NR_CHK_ORD AND CD_AER = @CD_AER

		UPDATE	T_CHK
		SET	NR_CHK_ORD = NR_CHK_ORD + 1
