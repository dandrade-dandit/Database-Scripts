<o:Trigger Id="o28">
<a:ObjectID>6958B6FA-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_DEP_TRD</a:Name>
<a:Code>T_DEP_TRD</a:Code>
<a:Text>Create Trigger T_DEP_TRD On T_DEP
For Delete As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Dados Estatísticos de Partida»Tarifas Pagas no Bilhete: Se Relaciona (Cascade) */
Delete T_TFB001 From T_TFB001 dep, Deleted pri
 Where dep.CD_PAR = pri.CD_PAR
/* Regra de negócio */
/*
/* Não permitir a exclusão do registro de T_DEP de um determinado vôo, se existir registro na T_TFB001 relacionado a esse vôo */
IF	(SELECT	COUNT(*) 
	FROM 	T_TFB001 TFB, 
		DELETED DEL 
	WHERE 	TFB.CD_PAR = DEL.CD_PAR) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: Não é possível excluir os Dados Estatísticos de Partida de um vôo, se para esse vôo existe registros de Tarifas de Embarque Pagas no Bilhete.&#39;
	RETURN

END

*/
End
go</a:Text>
<a:CreationDate>1081543727</a:CreationDate>
<a:Creator>prsi_ronei</a:Creator>
<a:ModificationDate>1081543727</a:ModificationDate>
<a:Modifier>prsi_ronei</a:Modifier>
<a:Trigger.Type>D</a:Trigger.Type>
</o:Trigger>
<o:Trigger Id="o29">
<a:ObjectID>6958B6FD-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_DEP_TRI</a:Name>
<a:Code>T_DEP_TRI</a:Code>
<a:Text>Create Trigger T_DEP_TRI On T_DEP
For Insert As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Aeroporto»Dados Estatísticos de Partida: Está em */
If (Select Count(*) From T_AER, Inserted
Where T_AER.CD_AER = Inserted.CD_AER)
 &lt;&gt;(Select Count(CD_AER) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Aeroporto&gt; que se deseja incluir na tabela &lt;Dados Estatísticos de Partida&gt; não está cadastrada&#39;
  Return
End
/* Partidas»Dados Estatísticos de Partida: Dados Estatísticos de Partida */
If (Select Count(*) From T_PAR, Inserted
Where T_PAR.CD_PAR = Inserted.CD_PAR)
 &lt;&gt;(Select Count(CD_PAR) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Partidas&gt; que se deseja incluir na tabela &lt;Dados Estatísticos de Partida&gt; não está cadastrada&#39;
  Return
End
/* Regra de negócio */
/* Se todos os campos do registro que está sendo incluído forem nulos, deletar o registro */
DELETE	T_DEP
FROM	T_DEP DEP, INSERTED INS
WHERE	DEP.CD_PAR = INS.CD_PAR AND
	INS.QT_DEP_TFB_DOM IS NULL AND
	INS.QT_DEP_TFB_INT IS NULL AND
	INS.QT_DEP_TFC_DOM IS NULL AND 
	INS.QT_DEP_TFC_INT IS NULL AND
	INS.QT_DEP_ISE_COL IS NULL AND
	INS.QT_DEP_ISE_TRA IS NULL AND
	INS.QT_DEP_ISE_CON_DOM IS NULL AND
	INS.QT_DEP_ISE_CON_INT IS NULL AND
	INS.QT_DEP_ISE_OUT IS NULL AND
	INS.QT_DEP_EMB_BAG_DOM IS NULL AND
	INS.QT_DEP_EMB_BAG_INT IS NULL AND
	INS.QT_DEP_EMB_CAR_DOM IS NULL AND
	INS.QT_DEP_EMB_CAR_INT IS NULL AND
	INS.QT_DEP_EMB_COR_DOM IS NULL AND
	INS.QT_DEP_EMB_COR_INT IS NULL

/* Não permitir que se inclua um registro em T_DEP com o campo QT_DEP_TFB_DOM diferente de Null ou diferente de 0 (zero) */
IF 	(SELECT COUNT (*)
	FROM	INSERTED
	WHERE	ISNULL(QT_DEP_TFB_DOM, 0) &lt;&gt; 0) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: Valor Inválido para o campo Tarifas Domésticas Pagas no Bilhete.&#39;
	RETURN
END

/* Não permitir que se inclua um registro em T_DEP com o campo QT_DEP_TFB_INT diferente de Null ou diferente de 0 (zero) */
IF 	(SELECT COUNT (*)
	FROM	INSERTED
	WHERE	ISNULL(QT_DEP_TFB_INT, 0) &lt;&gt; 0) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: Valor Inválido para o campo Tarifas Internacionais Pagas no Bilhete.&#39;
	RETURN
END

/* Não permitir Informações Internacionais para Vôos Regionais */
IF 	(SELECT COUNT (*)
	FROM	INSERTED INS, T_PAR PAR
	WHERE	PAR.CD_PAR = INS.CD_PAR AND
		PAR.TP_PAR = 3 AND
		(ISNULL(INS.QT_DEP_TFB_INT, 0) &lt;&gt; 0 OR
		INS.QT_DEP_TFC_INT IS NOT NULL OR
		INS.QT_DEP_ISE_CON_INT IS NOT NULL OR
		INS.QT_DEP_EMB_BAG_INT IS NOT NULL OR
		INS.QT_DEP_EMB_CAR_INT IS NOT NULL OR 
		INS.QT_DEP_EMB_COR_INT IS NOT NULL) ) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: Não é permitido Informações Internacionais para Vôos Regionais.&#39;
	RETURN
END

End
go</a:Text>
