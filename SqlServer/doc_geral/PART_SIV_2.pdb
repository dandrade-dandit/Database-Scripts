<a:ObjectID>6958B853-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_TFB001_TRD</a:Name>
<a:Code>T_TFB001_TRD</a:Code>
<a:Text>Create Trigger T_TFB001_TRD On T_TFB001
For Delete As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Regra de negócio */
/* Atualizar os campos QT_DEP_TFB_DOM e QT_DEP_TFB_INT da tabela T_DEP, de acordo com as quantidades 
excluídas na T_TFB001, levando em conta o Tipo de Tarifa de Embarque (Doméstico ou Internacional) associada 
ao campo CD_HTE do registro que estará sendo excluído */

/* Declaração do cursor */
DECLARE TFB_DEL CURSOR FOR
SELECT 	DISTINCT CD_PAR
FROM	DELETED

/* Declaração das variáveis que receberão o cursor */
DECLARE 	@CD_PAR		INT,
		@QT_TFB_DOM	 	INT,
		@QT_TFB_INT  	 	INT

/* Abrir cursor */
OPEN TFB_DEL

/* Ir para o primeiro registro */
FETCH	TFB_DEL 
INTO	@CD_PAR

/* Processar todos os registros de TFB_DEL */
WHILE @@FETCH_STATUS = 0
BEGIN
	SELECT @QT_TFB_DOM = 0, @QT_TFB_INT = 0

	/* Tarifas de Embarque Pagas no Bilhete Domésticas */
	SELECT 	@QT_TFB_DOM = SUM(QT_TFB) 
	FROM 	DELETED DEL, T_HTE HTE, T_TFE TFE  
	WHERE 	HTE.CD_HTE = DEL.CD_HTE AND 
		TFE.CD_TFE = HTE.CD_TFE 
	GROUP BY CD_PAR, TP_TFE 
	HAVING 	CD_PAR = @CD_PAR AND 
		TP_TFE = 1

	/* Tarifas de Embarque Pagas no Bilhete Internacionais */
	SELECT 	@QT_TFB_INT = SUM(QT_TFB) 
	FROM 	DELETED DEL, T_HTE HTE, T_TFE TFE  
	WHERE 	HTE.CD_HTE = DEL.CD_HTE AND 
		TFE.CD_TFE = HTE.CD_TFE 
	GROUP BY CD_PAR, TP_TFE 
	HAVING 	CD_PAR = @CD_PAR AND 
		TP_TFE = 2

	UPDATE	T_DEP
	SET	QT_DEP_TFB_DOM = ISNULL(QT_DEP_TFB_DOM, 0) - @QT_TFB_DOM,
		QT_DEP_TFB_INT = ISNULL(QT_DEP_TFB_INT, 0) - @QT_TFB_INT
	WHERE	CD_PAR = @CD_PAR

	/* Ir para o próximo registro */
	FETCH NEXT FROM TFB_DEL 
	INTO	@CD_PAR
END

CLOSE TFB_DEL
DEALLOCATE TFB_DEL

End
go</a:Text>
