<o:Trigger Id="o204">
<a:ObjectID>6958B857-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_TFB001_TRU</a:Name>
<a:Code>T_TFB001_TRU</a:Code>
<a:Text>Create Trigger T_TFB001_TRU On T_TFB001
For Update As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
If Update (CD_TFB)
Begin
  RollBack Transaction
  RaisError 20002 &#39;Operação não executada: A informação &lt;Código da Tarifa Paga no Bilhete&gt; da tabela &lt;Tarifas Pagas no Bilhete&gt; não pode ser alterada.&#39;
  Return
End
If Update (CD_COM)
/* Companhia Aérea»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_COM, Inserted
Where T_COM.CD_COM = Inserted.CD_COM)
 &lt;&gt;(Select Count(CD_COM) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Companhia Aérea&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
If Update (CD_HTE)
/* Histórico de Tarifas de Embarque»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_HTE, Inserted
Where T_HTE.CD_HTE = Inserted.CD_HTE)
 &lt;&gt;(Select Count(CD_HTE) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Histórico de Tarifas de Embarque&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
If Update (CD_PAR)
/* Dados Estatísticos de Partida»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_DEP, Inserted
Where T_DEP.CD_PAR = Inserted.CD_PAR)
 &lt;&gt;(Select Count(CD_PAR) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Dados Estatísticos de Partida&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
/* Regra de negócio */
/* A Categoria do Aeroporto da Tarifa de Embarque tem que ser igual a Categoria do Aeroporto do Resumo de Passageiros Embarcados */
IF (	SELECT COUNT(*)
	FROM 	INSERTED INS, 
		T_HTE HTE, 
		T_TFE TFE, 
		T_DEP DEP, 
		T_AER AER
	WHERE	DEP.CD_PAR = INS.CD_PAR AND
		AER.CD_AER = DEP.CD_AER AND
		HTE.CD_HTE = INS.CD_HTE AND 
		TFE.CD_TFE = HTE.CD_TFE AND
		AER.TP_AER_CTG &lt;&gt; TFE.TP_TFE_AER_CTG ) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: A Categoria do Aeroporto da Tarifa de Embarque tem que ser a igual a Categoria do Aeroporto do RPE associado.&#39;
	RETURN
END

/* Não permitir que se altere o campo CD_HTE para um outro CD_HTE que 
tenha o Tipo de Tarifa de Embarque associado diferente do valor original. */
IF UPDATE(CD_HTE)
BEGIN
	IF (	SELECT 	COUNT(*)
		FROM 	INSERTED INS,
			DELETED DEL,
			T_HTE HTEINS, 
			T_HTE HTEDEL, 
			T_TFE TFEINS, 
			T_TFE TFEDEL
		WHERE	INS.CD_TFB = DEL.CD_TFB AND
			HTEINS.CD_HTE = INS.CD_HTE AND
			TFEINS.CD_TFE = HTEINS.CD_TFE AND
			HTEDEL.CD_HTE = DEL.CD_HTE AND
			TFEDEL.CD_TFE = HTEDEL.CD_TFE AND
			TFEINS.TP_TFE &lt;&gt; TFEDEL.TP_TFE) &gt; 0
	BEGIN
		ROLLBACK TRANSACTION
		RAISERROR 20001 &#39;Operação não executada: O Tipo de Tarifa de Embarque associado, não pode ser diferente do valor original.&#39;
		RETURN
	END
END

/* Atualizar os campos QT_DEP_TFB_DOM e QT_DEP_TFB_INT da tabela T_DEP, de acordo com os valores do campo QT_TFB
da tabela T_TFB001, levando em conta o Tipo de Tarifa de Embarque (Doméstico ou Internacional) associada ao campo CD_HTE 
do registro que estará sendo alterado*/
IF 	UPDATE(QT_TFB)
BEGIN

	/* Declaração do cursor */

	DECLARE TFB_UPD CURSOR FOR
	SELECT 	DISTINCT INS.CD_PAR
	FROM	INSERTED INS, DELETED DEL
	WHERE	DEL.CD_TFB = INS.CD_TFB

	/* Declaração das variáveis que receberão o cursor */
	DECLARE 	@CD_PAR		INT,
			@QT_TFB_DOM_INS 	INT,
			@QT_TFB_INT_INS 	INT,
			@QT_TFB_DOM_DEL 	INT,
			@QT_TFB_INT_DEL 	INT

	/* Abrir cursor */
	OPEN TFB_UPD

	/* Ir para o primeiro registro */
	FETCH	TFB_UPD
	INTO	@CD_PAR

	/* Processar todos os registros de TFB_UPD */
	WHILE @@FETCH_STATUS = 0
	BEGIN
		SELECT @QT_TFB_DOM_INS = 0, @QT_TFB_INT_INS = 0, @QT_TFB_DOM_DEL = 0, @QT_TFB_INT_DEL = 0

		/* Tarifas de Embarque Pagas no Bilhete Domésticas da Tabela Inserted */
		SELECT 	@QT_TFB_DOM_INS = SUM(QT_TFB)
		FROM 	INSERTED INS, T_HTE HTE, T_TFE TFE
		WHERE	HTE.CD_HTE = INS.CD_HTE AND
			TFE.CD_TFE = HTE.CD_TFE 
		GROUP BY CD_PAR, TP_TFE
		HAVING	CD_PAR = @CD_PAR AND
			TP_TFE = 1

		/* Tarifas de Embarque Pagas no Bilhete Internacionais da Tabela Inserted */
		SELECT 	@QT_TFB_INT_INS = SUM(QT_TFB)
		FROM 	INSERTED INS, T_HTE HTE, T_TFE TFE
		WHERE	HTE.CD_HTE = INS.CD_HTE AND
			TFE.CD_TFE = HTE.CD_TFE 
		GROUP BY CD_PAR, TP_TFE
		HAVING	CD_PAR = @CD_PAR AND
			TP_TFE = 2

		/* Tarifas de Embarque Pagas no Bilhete Domésticas da Tabela Deleted */
		SELECT 	@QT_TFB_DOM_DEL = SUM(QT_TFB)
		FROM 	DELETED DEL, T_HTE HTE, T_TFE TFE
		WHERE	HTE.CD_HTE = DEL.CD_HTE AND
			TFE.CD_TFE = HTE.CD_TFE 
		GROUP BY CD_PAR, TP_TFE
		HAVING	CD_PAR = @CD_PAR AND
			TP_TFE = 1

		/* Tarifas de Embarque Pagas no Bilhete Internacionais da Tabela Deleted */
		SELECT 	@QT_TFB_INT_DEL = SUM(QT_TFB)
		FROM 	DELETED DEL, T_HTE HTE, T_TFE TFE
		WHERE	HTE.CD_HTE = DEL.CD_HTE AND
			TFE.CD_TFE = HTE.CD_TFE 
		GROUP BY CD_PAR, TP_TFE
		HAVING	CD_PAR = @CD_PAR AND
			TP_TFE = 2

		IF	@QT_TFB_DOM_DEL &lt;&gt; @QT_TFB_DOM_INS
		BEGIN
			UPDATE	T_DEP 
			SET	QT_DEP_TFB_DOM = ISNULL(QT_DEP_TFB_DOM, 0) - @QT_TFB_DOM_DEL + @QT_TFB_DOM_INS
			WHERE	CD_PAR = @CD_PAR
		END

		IF	@QT_TFB_INT_DEL &lt;&gt; @QT_TFB_INT_INS
		BEGIN
			UPDATE	T_DEP 
			SET	QT_DEP_TFB_INT = ISNULL(QT_DEP_TFB_INT, 0) - @QT_TFB_INT_DEL + @QT_TFB_INT_INS
			WHERE	CD_PAR = @CD_PAR
		END

		/* Ir para o próximo registro */
		FETCH NEXT FROM TFB_UPD
		INTO	@CD_PAR


	END
	CLOSE TFB_UPD
	DEALLOCATE TFB_UPD
END


End
go</a:Text>
