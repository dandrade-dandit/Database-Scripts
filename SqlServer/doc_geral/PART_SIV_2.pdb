</o:Trigger>
<o:Trigger Id="o30">
<a:ObjectID>6958B6FF-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_DEP_TRU</a:Name>
<a:Code>T_DEP_TRU</a:Code>
<a:Text>Create Trigger T_DEP_TRU On T_DEP
For Update As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
If Update (CD_PAR)
Begin
  RollBack Transaction
  RaisError 20002 &#39;Operação não executada: A informação &lt;Código do  Vôo de Partida&gt; da tabela &lt;Dados Estatísticos de Partida&gt; não pode ser alterada.&#39;
  Return
End
If Update (CD_AER)
/* Aeroporto»Dados Estatísticos de Partida: Está em */
If (Select Count(*) From T_AER, Inserted
Where T_AER.CD_AER = Inserted.CD_AER)
 &lt;&gt;(Select Count(CD_AER) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Aeroporto&gt; que se deseja incluir na tabela &lt;Dados Estatísticos de Partida&gt; não está cadastrada&#39;
  Return
End
/* Regra de negócio */
/* Se todos os campos do registro que está sendo alterado forem nulos, deletar o registro */
DELETE	T_DEP
FROM	T_DEP DEP, INSERTED INS
WHERE	DEP.CD_PAR = INS.CD_PAR AND
	INS.QT_DEP_TFB_DOM IS NULL AND
	INS.QT_DEP_TFB_INT IS NULL AND
	INS.QT_DEP_TFC_DOM IS NULL AND 
	INS.QT_DEP_TFC_INT IS NULL AND
	INS.QT_DEP_ISE_COL IS NULL AND
	INS.QT_DEP_ISE_TRA IS NULL AND
	INS.QT_DEP_ISE_CON_DOM IS NULL AND
	INS.QT_DEP_ISE_CON_INT IS NULL AND
	INS.QT_DEP_ISE_OUT IS NULL AND
	INS.QT_DEP_EMB_BAG_DOM IS NULL AND
	INS.QT_DEP_EMB_BAG_INT IS NULL AND
	INS.QT_DEP_EMB_CAR_DOM IS NULL AND
	INS.QT_DEP_EMB_CAR_INT IS NULL AND
	INS.QT_DEP_EMB_COR_DOM IS NULL AND
	INS.QT_DEP_EMB_COR_INT IS NULL

/* Não permitir Informações Internacionais para Vôos Regionais */
IF 	(SELECT COUNT (*)
	FROM	INSERTED INS, T_PAR PAR
	WHERE	PAR.CD_PAR = INS.CD_PAR AND
		PAR.TP_PAR = 3 AND
		(ISNULL(INS.QT_DEP_TFB_INT, 0) &lt;&gt; 0 OR
		INS.QT_DEP_TFC_INT IS NOT NULL OR
		INS.QT_DEP_ISE_CON_INT IS NOT NULL OR
		INS.QT_DEP_EMB_BAG_INT IS NOT NULL OR
		INS.QT_DEP_EMB_CAR_INT IS NOT NULL OR 
		INS.QT_DEP_EMB_COR_INT IS NOT NULL) ) &gt; 0
BEGIN
	RollBack Transaction
	RaisError 20001 &#39;Operação não executada: Não é permitido Informações Internacionais para Vôos Regionais.&#39;
	Return
END

/* Verificar se os valores dos campos de Tarifas de Embarque estão consistentes */
IF 	UPDATE(QT_DEP_TFB_DOM) OR UPDATE(QT_DEP_TFB_INT)
BEGIN
	/* Declaração do cursor */
	DECLARE DEP_UPD CURSOR FOR
	SELECT	DISTINCT DEP.CD_PAR, ISNULL(DEP.QT_DEP_TFB_DOM, 0), ISNULL(DEP.QT_DEP_TFB_INT, 0)
	FROM	T_DEP DEP,
		INSERTED INS
	WHERE	DEP.CD_PAR = INS.CD_PAR

	/* Declaração das variáveis que receberão o cursor */
	DECLARE 	@CD_PAR		INT,
			@QT_DEP_TFB_DOM	INT,
			@QT_DEP_TFB_INT	INT,
			@QT_TFB_DOM		INT,
			@QT_TFB_INT		INT


	/* Abrir cursor */
	OPEN DEP_UPD

	/* Ir para o primeiro registro */
	FETCH	DEP_UPD 
	INTO	@CD_PAR,
		@QT_DEP_TFB_DOM,
		@QT_DEP_TFB_INT

	/* Processar todos os registros de DEP_UPD */
	WHILE @@FETCH_STATUS = 0
	BEGIN
		SELECT @QT_TFB_DOM = 0, @QT_TFB_INT = 0



		/* Tarifas de Embarque Pagas no Bilhete Domésticas */
		SELECT	@QT_TFB_DOM = SUM(QT_TFB)

		FROM 	T_TFB001 TFB, 
			T_HTE HTE, 
			T_TFE TFE
		WHERE 	TFB.CD_PAR = @CD_PAR AND 

			HTE.CD_HTE = TFB.CD_HTE AND 
			TFE.CD_TFE = HTE.CD_TFE 
		GROUP BY TFB.CD_PAR, TP_TFE
		HAVING TFE.TP_TFE = 1

		/* Tarifas de Embarque Pagas no Bilhete Internacionais */
		SELECT @QT_TFB_INT = SUM(QT_TFB)
		FROM 	T_TFB001 TFB, 
			T_HTE HTE, 
			T_TFE TFE
		WHERE 	TFB.CD_PAR = @CD_PAR AND 
			HTE.CD_HTE = TFB.CD_HTE AND 
			TFE.CD_TFE = HTE.CD_TFE
		GROUP BY TFB.CD_PAR, TP_TFE
		HAVING TFE.TP_TFE = 2 

		IF @QT_DEP_TFB_DOM  &lt;&gt; @QT_TFB_DOM

		BEGIN
			ROLLBACK TRANSACTION
			RAISERROR 20001 &#39;Operação não executada: Dados de Tarifas Domésticas Pagas no Bilhete ou Dados de Tarifas Domésticas Pagas no Bilhete por Companhia Aérea estão inconsistentes.&#39; 
			RETURN
		END

		IF @QT_DEP_TFB_INT  &lt;&gt; @QT_TFB_INT

		BEGIN
			ROLLBACK TRANSACTION
			RAISERROR 20001 &#39;Operação não executada: Dados de Tarifas Internacionais Pagas no Bilhete ou Dados de Tarifas Internacionais Pagas no Bilhete por Companhia Aérea estão inconsistentes.&#39;
			RETURN

		END


		/* Ir para o próximo registro */
		FETCH NEXT FROM DEP_UPD
		INTO	@CD_PAR,
			@QT_DEP_TFB_DOM,
			@QT_DEP_TFB_INT
	END



	CLOSE DEP_UPD
	DEALLOCATE DEP_UPD
END

End
