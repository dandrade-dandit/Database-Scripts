<o:Trigger Id="o203">
<a:ObjectID>6958B855-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_TFB001_TRI</a:Name>
<a:Code>T_TFB001_TRI</a:Code>
<a:Text>Create Trigger T_TFB001_TRI On T_TFB001
For Insert As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Companhia Aérea»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_COM, Inserted
Where T_COM.CD_COM = Inserted.CD_COM)
 &lt;&gt;(Select Count(CD_COM) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Companhia Aérea&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
/* Histórico de Tarifas de Embarque»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_HTE, Inserted
Where T_HTE.CD_HTE = Inserted.CD_HTE)
 &lt;&gt;(Select Count(CD_HTE) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Histórico de Tarifas de Embarque&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
/* Dados Estatísticos de Partida»Tarifas Pagas no Bilhete: Se Relaciona */
If (Select Count(*) From T_DEP, Inserted
Where T_DEP.CD_PAR = Inserted.CD_PAR)
 &lt;&gt;(Select Count(CD_PAR) From Inserted)
Begin
  RollBack Transaction
  RaisError 20001 &#39;Operação não executada: A informação &lt;Dados Estatísticos de Partida&gt; que se deseja incluir na tabela &lt;Tarifas Pagas no Bilhete&gt; não está cadastrada&#39;
  Return
End
/* Regra de negócio */
/* Não permitir a inclusão de TFB001 para um determinado vôo se não houver registro em T_DEP relacionado ao mesmo vôo */
IF	(SELECT	COUNT(*)
	FROM	T_DEP DEP,
		INSERTED INS
	WHERE	DEP.CD_PAR = INS.CD_PAR) = 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: Não é possível incluir registros de Tarifas de Embarque Pagas no Bilhete para um determinado vôo, se para esse vôo não existe registro de Dados Estatísticos de Partida.&#39;
	RETURN

END

/* A Categoria do Aeroporto da Tarifa de Embarque tem que ser igual a Categoria do Aeroporto do Resumo de Passageiros Embarcados */
IF (	SELECT 	COUNT(*)
	FROM 	INSERTED INS, 
		T_HTE HTE, 
		T_TFE TFE, 
		T_DEP DEP, 
		T_AER AER
	WHERE	DEP.CD_PAR = INS.CD_PAR AND
		AER.CD_AER = DEP.CD_AER AND
		HTE.CD_HTE = INS.CD_HTE AND 
		TFE.CD_TFE = HTE.CD_TFE AND
		AER.TP_AER_CTG &lt;&gt; TFE.TP_TFE_AER_CTG ) &gt; 0
BEGIN
	ROLLBACK TRANSACTION
	RAISERROR 20001 &#39;Operação não executada: A Categoria do Aeroporto da Tarifa de Embarque tem que ser a igual a Categoria do Aeroporto do RPE associado.&#39;
	RETURN
END

/* Atualizar os campos QT_DEP_TFB_DOM e QT_DEP_TFB_INT da tabela T_DEP, de acordo com as quantidades incluídas na T_TFB001 */
UPDATE	DEP
SET	QT_DEP_TFB_DOM = ISNULL(QT_DEP_TFB_DOM, 0) + INS.QT_TFB
FROM	T_DEP DEP, INSERTED INS, T_HTE HTE, T_TFE TFE
WHERE	DEP.CD_PAR = INS.CD_PAR AND
	HTE.CD_HTE = INS.CD_HTE AND
	TFE.CD_TFE = HTE.CD_TFE AND
	TFE.TP_TFE = 1

UPDATE	DEP
SET	QT_DEP_TFB_INT = ISNULL(QT_DEP_TFB_INT, 0) + INS.QT_TFB
FROM	T_DEP DEP, INSERTED INS, T_HTE HTE, T_TFE TFE
WHERE	DEP.CD_PAR = INS.CD_PAR AND
	HTE.CD_HTE = INS.CD_HTE AND
	TFE.CD_TFE = HTE.CD_TFE AND
	TFE.TP_TFE = 2

End
go</a:Text>
