<o:Trigger Id="o239">
<a:ObjectID>6958B7C8-8FBB-11D8-B6CA-00024416849C</a:ObjectID>
<a:Name>T_HSP_TRD</a:Name>
<a:Code>T_HSP_TRD</a:Code>
<a:Text>Create Trigger T_HSP_TRD On T_HSP
For Delete As
Begin
IF @@ROWCOUNT = 0 RETURN
SET NOCOUNT ON
/* Regra de negócio */
/* Não permitir a deleção de vôos que tenham mais de um histórico de situação
    (Fazer a checagem somente se esse vôo não existir mais na tabela T_PAR) */
IF (	SELECT 	COUNT(*)
	FROM	DELETED DEL
	WHERE	EXISTS (SELECT CD_PAR FROM T_PAR WHERE CD_PAR = DEL.CD_PAR)) = 0
BEGIN
	IF (	SELECT	COUNT(*) 
		FROM 	DELETED DEL
		WHERE	EXISTS	(SELECT	COUNT(*)
				FROM	DELETED
				GROUP BY CD_PAR
				HAVING 	CD_PAR = DEL.CD_PAR AND
					COUNT(*) &gt; 1) 	) &gt; 0
	BEGIN
		RAISERROR 20999 &#39;Operação não executada: Este vôo já possui movimentação de situação e não pode ser excluído.&#39;
		ROLLBACK TRANSACTION
		RETURN
	END
END

End
go</a:Text>
